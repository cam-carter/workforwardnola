<div class="jobCardContainer" id="career-list">
  <h1>Your career assessment results</h1>
  {{#career_descriptions}}
    <div class="jobCard">
      <h2>{{job_title}}</h2>
      <h3>About <span class="wage">{{experienced_wage}}/hour</span> after training</h3>
      <p>{{job_description}}</p>

      {{#general_duties}}
      <h3>General Duties</h3>
      <div class="general-duties">
        {{& general_duties }}
      </div>
      {{/general_duties}}

      {{#training}}
      <h3>Training</h3>
      <div class="training">
        {{& training }}
      </div>
      {{/training}}
    </div>
  {{/career_descriptions}}
  <div class="clearfix"></div>
</div>
